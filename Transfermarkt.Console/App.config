<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <add key="BaseURL" value="https://www.transfermarkt.pt" />
    <add key="BaseFolderPath" value="c:\Transfermarkt" />
    <add key="Level1FolderFormat" value="TM_{0}" />
    <add key="SimpleClubUrlFormat" value="/{CLUB_STRING}/startseite/verein/{CLUB_ID}/saison_id/{SEASON}" />
    <add key="PlusClubUrlFormat" value="/{CLUB_STRING}/kader/verein/{CLUB_ID}/saison_id/{SEASON}/plus/1" />
    <add key="IdentifiersGetterPattern" value="{([a-zA-Z_0-9]*)}" />
    <add key="IdentifiersSetterPattern" value="(?&lt;{ID}&gt;.*)" />
  </appSettings>
</configuration>